pipeline {
  agent any

  stages {
    stage('Fetch Source Code') {
        steps{
            dir('repo') {
                git branch: "${params.Branch}", credentialsId: 'Creds', url: "${params.RepoURL}"
            }
        }
    }
    stage('Build') {
      steps {
        // Use sh step to execute a shell command
        sh 'cp -r Baarish.mp3 Alone.flac /Users/simran/Downloads/Code/test' // This copies all files from current directory to a new directory named "target"
        echo "Building..."
      }
    }
  }
}
