pipeline {
    agent any

    stages {
        stage('Copy Files') {
            steps {
                git branch: 'master', 
                     credentialsId: 'Creds', 
                     url: 'https://github.com/SimranBanwait/Background_Generator.github.io.git' 

                def filesToCopy = [
                    'Alone.flac',
                ]
                for (file in filesToCopy) {
                    sh "Alone.flac /Users/simran/Downloads/Code/test/"
                }
            }
        }
    }
}
